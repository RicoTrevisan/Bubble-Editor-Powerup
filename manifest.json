{
  "manifest_version": 3,
  "name": "Bubble Page CSS Injector",
  "icons": {
   "21": "Codeless Love 21px.png",
   "64": "Codeless Love 64px.png",
   "128": "Codeless Love 128px.png"
 },
  "version": "1.0",
  "description": "Inject custom CSS into Bubble.io editor page",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["https://bubble.io/page*"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["style.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
